{
  "name": "movies-freak-server",
  "version": "1.0.0",
  "description": "An app to add movies, tv shows and create watchlists",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "lint": "eslint .",
    "test:ci": "NODE_ENV=ci yarn ts-node jesusx21/classpuccino/cli tests/src",
    "test:all": "NODE_ENV=test yarn ts-node jesusx21/classpuccino/cli tests/src",
    "test": "NODE_ENV=test yarn ts-node jesusx21/classpuccino/cli ",
    "migrate:make": "knex migrate:make $1",
    "migrate:testing": "knex migrate:latest --env test",
    "migrate:ci": "knex migrate:latest --env ci",
    "migrate:latest": "knex migrate:latest --env develop",
    "migrate:rollback": "knex migrate:rollback --env=develop && knex migrate:rollback --env test",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "start:dev": "NODE_ENV=develop nodemon",
    "serve": "webpack serve"
  },
  "repository": "git@github.com:jesusx21/movies-freak-server.git",
  "author": "Jesús Agustín Peña Meza",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "clone-deep": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "jsonschema": "^1.4.1",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.2",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "triple-beam": "^1.4.1",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/knex-cleaner": "^1.3.6",
    "@types/lodash": "^4.17.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.0.1",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@webpack-cli/generators": "^3.0.7",
    "colors": "^1.4.0",
    "core-js": "3",
    "knex-cleaner": "^1.3.1",
    "sinon": "^17.0.1",
    "source-map-loader": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-node-externals": "^3.0.0",
    "webpack": "^5.95.0"
  }
}
